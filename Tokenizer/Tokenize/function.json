{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
 	{
      "name": "outputblob",
      "type": "blob",
      "dataType": "binary",
      "path": "tokens/test.json",
      "connection": "MyStorageConnectionAppSetting",
      "direction": "out"
    }
  ]
}
